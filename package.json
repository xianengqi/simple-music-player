{
  "name": "simple-music-player",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "nodemon --watch main.js --exec 'electron .'",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "simpleMusicPlayerXiaNengQi",
    "mac": {
      "category": "public.app-category.productivity"
    },
    "dmg": {
      "background": "build/appdmg.png",
      "icon": "build/icon.icns",
      "iconSize": 100,
      "contents": [
        {
          "x": 380,
          "y": 280,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 110,
          "y": 280,
          "type": "file"
        }
      ],
      "window": {
        "width": 500,
        "height": 500
      }
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "squirrel",
      "icon": "build/icon.ico"
    }
  },
  "keywords": [],
  "author": "XiaNengQi",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^4.4.1",
    "electron-store": "^5.1.1",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "electron": "^11.5.0",
    "electron-builder": "^22.4.1"
  }
}
